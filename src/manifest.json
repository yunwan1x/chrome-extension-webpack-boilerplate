{
  "name": "Chrome Extension Webpack",
  "background": {
    "scripts": ["background.js"]
  },

  "icons": {
    "128": "icon-128.png"
  },
  "chrome_url_overrides" : {
    "newtab": "popup.html"
  },
  "permissions":
  [
    "tabs",
    "bookmarks",
    "unlimitedStorage",
    "chrome://favicon/",
    "chrome://favicon/*"





  ],
  "version": "1.0",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
