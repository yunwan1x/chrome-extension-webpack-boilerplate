{
  "name": "Chrome Extension Webpack",
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "",
    "default_icon": "icon-128.png"
  },
  "offline_enabled": true,
  "icons": {
    "128": "icon-128.png"
  },
  "permissions":
  [
    "tabs",
    "bookmarks",
    "unlimitedStorage",
    "chrome://favicon/",
    "*://*/*",
    "tabs",
    "storage",
    "history",
    "management",
    "webRequest",
    "webRequestBlocking"
  ],
  "version": "1.0",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
